# Local PostgreSQL configuration
POSTGRES_PASSWORD=dev_password
POSTGRES_USER=postgres
POSTGRES_DB=survey_dev

# Backend configuration for local PostgreSQL
DATABASE_URL=postgresql://postgres:dev_password@localhost:5432/survey_dev
SUPABASE_URL=http://localhost:5432
SUPABASE_PUBLISHABLE_KEY=local-dev-key
SUPABASE_SECRET_KEY=local-dev-secret

# Copy these from your .env file
OPENAI_API_KEY=${OPENAI_API_KEY}
LANGCHAIN_API_KEY=${LANGCHAIN_API_KEY}
TAVILY_API_KEY=${TAVILY_API_KEY}

# Backend port
BACKEND_PORT=8000
FRONTEND_PORT=3000

# Environment
ENVIRONMENT=development
LOG_LEVEL=DEBUG