# Docker Local Development Environment Variables
# Copy this file to .env and fill in your values

# ============================================
# REQUIRED: Configuration
# ============================================
# OpenAI API Key
OPENAI_API_KEY=

# Supabase Configuration
SUPABASE_URL=
SUPABASE_PUBLISHABLE_KEY=
SUPABASE_SECRET_KEY=

# Tool Use Keys
GOOGLE_MAPS_API_KEY=
TAVILY_API_KEY=

# LangChain tracing (optional)
LANGCHAIN_API_KEY=

# ============================================
# Docker Configuration (usually leave as default)
# ============================================
# Port configuration (change if ports conflict)
BACKEND_PORT=8000
FRONTEND_PORT=5173
ADMIN_PORT=5174

# ============================================
# Application Configuration
# ============================================
# Environment
ENVIRONMENT=development
LOG_LEVEL=DEBUG
